{
    "name": "Local storage + WASM test",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "A Chromium extension for local storage + WASM test.",
    "permissions": [
        "http://*/", "https://*/"
        ],    
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
